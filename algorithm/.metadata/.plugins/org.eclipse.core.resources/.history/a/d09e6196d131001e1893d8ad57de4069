package strangePrime_2023;

import java.util.Scanner;

public class Main {
	
	public static void isPrime(int n, int cnt, int N) {
		int where = N;
		if (n%2 == 0 && n != 2) return;
		for(int i = 3; i*i <= now; i+=2) {
			if (now%i == 0) return false;
		}
		
		return true;
	}
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		StringBuffer sb = new StringBuffer();
		int N = sc.nextInt();
		int cnt = 1;
		for (int n = 2; n < 10; n++) {
			isPrime(n, cnt, N);
			if(isPrime(n, N)) {
				sb.append(n).append("\n");
			}
		}
		System.out.println(sb);
	}

}
