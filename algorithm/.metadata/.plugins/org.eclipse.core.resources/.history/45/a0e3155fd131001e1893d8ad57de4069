package strangePrime_2023;

import java.util.Scanner;

public class Main {
	
	public static boolean isPrime(int n, int cnt, int N) {
		int where = N;
		int cnt;
		while(where > 0) {
			
			if (now%2 == 0 && now != 2) return false;
			for(int i = 3; i*i <= now; i+=2) {
				if (now%i == 0) return false;
			}
			where--;
		}
		return true;
	}
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		StringBuffer sb = new StringBuffer();
		int N = sc.nextInt();
		int cnt = 0;
		for (int n = 2; n < 10; n++) {
			if(isPrime(n, N)) {
				sb.append(n).append("\n");
			}
		}
		System.out.println(sb);
	}

}
