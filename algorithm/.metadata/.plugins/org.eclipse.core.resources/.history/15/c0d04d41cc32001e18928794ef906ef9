package planetX3_2830;

import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class Main {
	
	public static int sum = 0;
	
	public static void calc(int[] people, int start, int first, int second, int cnt) {
		
		
		if(cnt == 2) {
			int big;
			int small;
			if (people[first] >= people[second]) {
				big = people[first];
				small = people[second];
			} else {
				big = people[second];
				small = people[first];
			}
//			int big = people[start] >= people[chosen] ? people[start]:people[chosen];
//			int small = people[start] < people[chosen] ? people[start]:people[chosen];
			sum += (big^small);
			return;
		}
		
		for(int i = start; i < people.length; i++) {
			if(cnt == 0) {
				first = i;
			}
			calc(people, i+1, first, i, cnt+1);
			calc(people, i+1, i+1, i+1, cnt);
		}
	}

	public static void main(String[] args) throws NumberFormatException, IOException {

		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		int N = Integer.parseInt(br.readLine());
		int[] people = new int[N];
		for (int i = 0; i < N; i++) {
			people[i] = Integer.parseInt(br.readLine());
		}
		if (N==1) {
			System.out.println(sum);
			return;
		}
		calc(people, 0, 0, 0);
		System.out.println(sum);
	}

}
