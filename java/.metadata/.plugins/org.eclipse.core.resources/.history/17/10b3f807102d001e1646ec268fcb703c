package com.ssafy.trip.model.dto;

public class FestivalDto {

  /** 축제 식별 번호 */
  private int num;
  /** 광역 단체명 (Regional Local Government)*/
  private String RLG;
  /** 기초 단체명 (Basic Local Government)*/
  private String BLG;
  /** 축제명 */
  private String lotAddress;
  /** 축제유형 */
  /** 개최기간 */
  /** 개최장소 */
  private String info;
  /** 개최방식 */
  private String tel;
  /** 담당자 연락처 */
  private String img;

}
