package com.ssafy.trip.model.dto;

public class FestivalDto {

  /** 축제 식별 번호 */
  private int num;
  /** 광역 단체명 (Regional Local Government) */
  private String RLG;
  /** 기초 단체명 (Basic Local Government) */
  private String BLG;
  /** 축제명 */
  private String festivalName;
  /** 축제유형 */
  private String festivalType;
  /** 개최기간 */
  private String period;
  /** 개최장소 */
  private String location;
  /** 개최방식 */
  private String openType;
  /** 담당자 연락처 */
  private String tel;

  public FestivalDto(String RLG) {
    this.RLG = RLG;
  }

  public int getNum() {
    return num;
  }

  public void setNum(int num) {
    this.num = num;
  }

  public String getRLG() {
    return RLG;
  }

  public void setRLG(String rLG) {
    RLG = rLG;
  }

  public String getBLG() {
    return BLG;
  }

  public void setBLG(String bLG) {
    BLG = bLG;
  }

  public String getFestivalName() {
    return festivalName;
  }

  public void setFestivalName(String festivalName) {
    this.festivalName = festivalName;
  }

  public String getFestivalType() {
    return festivalType;
  }

  public void setFestivalType(String festivalType) {
    this.festivalType = festivalType;
  }

  public String getPeriod() {
    return period;
  }

  public void setPeriod(String period) {
    this.period = period;
  }

  public String getLocation() {
    return location;
  }

  public void setLocation(String location) {
    this.location = location;
  }

  public String getOpenType() {
    return openType;
  }

  public void setOpenType(String openType) {
    this.openType = openType;
  }

  public String getTel() {
    return tel;
  }

  public void setTel(String tel) {
    this.tel = tel;
  }

  @Override
  public String toString() {
    return "FestivalDto [num=" + num + ", RLG=" + RLG + ", BLG=" + BLG + ", festivalName="
        + festivalName + ", festivalType=" + festivalType + ", period=" + period + ", location="
        + location + ", openType=" + openType + ", tel=" + tel + "]";
  }
}
